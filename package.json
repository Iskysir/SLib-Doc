{
  "name": "SLib-Doc",
  "version": "0.0.1",
  "scripts": {
    "build-doc": "rm -rf documentation && make html",
    "start": "cross-env NODE_ENV=production node ./build/index.js",
    "start-dev": "./node_modules/.bin/babel-node server/index.js",
    "deploy": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.config.js --display-error-details && npm run build-server && cross-env NODE_ENV=production node ./build/index.js",
    "build-server": "cross-env NODE_ENV=production ./node_modules/.bin/babel ./server --out-dir ./build",
    "lint": "eslint --ignore-path .gitignore client server",
    "heroku-postbuild": "cross-env NODE_ENV=production webpack --config webpack.config.js && cross-env NODE_ENV=production ./node_modules/.bin/babel ./server --out-dir ./lib",
    "clean": "rm -rf build && mkdir build",
    "fetch": "./node_modules/.bin/babel-node fixture/index.js"
  },
  "devDependencies": {
    "babel-preset-stage-3": "^6.17.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.1.3",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.3"
  },
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chalk": "^1.1.3",
    "child_process": "^1.0.2",
    "connect-history-api-fallback": "^1.3.0",
    "cors": "^2.8.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "dialog-polyfill": "^0.4.5",
    "express": "^4.14.0",
    "express-request-proxy": "^2.0.0",
    "express-session": "^1.14.2",
    "faker": "^3.1.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.9.0",
    "file-saver": "^1.3.3",
    "firebase": "^3.6.4",
    "github-webhook-handler": "^0.6.0",
    "html-entities": "^1.2.0",
    "html-webpack-plugin": "^2.24.1",
    "lodash": "^4.17.2",
    "mobile-detect": "^1.3.5",
    "moment": "^2.17.1",
    "mysql": "^2.13.0",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.2.1",
    "precss": "^1.4.0",
    "rc-collapse": "^1.6.12",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-icons": "^2.2.3",
    "react-mdl": "^1.9.0",
    "react-mdl-extra": "^1.4.2",
    "react-router": "^3.0.0",
    "react-swipeable": "^3.9.2",
    "style-loader": "^0.13.1",
